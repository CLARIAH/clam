$def with (system_id, system_name, system_description, url, projects)
<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="$url/static/interface.xsl"?>
<clamindex id="$system_id" name="$system_name">
    <description>$system_description</description>
    <projects>
    $for project in projects:
        <project>$project</project>
    </projects>
</clamindex>
